  <li class="list-group-item comment pl-0 pr-0" style="border: none !important;">
    <div class="d-flex w-100 pl-0">
      <% @profile_img = invitation.user.try(:profile)&.exists? ? image_tag(invitation.user.profile.url(:square), :class => "profile_img_small mr-2") : image_tag("profile_image.jpg", :class => "profile_img_small mr-2") %>
      <%= link_to(@profile_img, invitation.user, target: "_blank") %>
      <div class="li-content0 w-100">
        <p class="m-0">
          You sent an invite to <b><%= invitation.email %></b> <%= time_ago_in_words(invitation.created_at) %> ago
        </p>
        <div class="progress md-progress mt-3 pl-0">
          <div class="progress-bar pl-0" role="progressbar" style="width: 25%" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100"></div>
        </div>
        <p>Progress: <b><%= invitation.status %></b></p>
      </div>
    </div>
  </li>

  <div class="btn_cntnr mt-0 mb-0 pb-0 pt-3 pl-0" style="float: none; position: relative;">
    <%= submit_tag "Send Another Invite", onclick: "newInvite()", class: "newpost--new--profile mr-0 pl-4 pr-4 mb-0 submit_modal", id: "submit-comment-modal-button" %>
  </div>
</div>

<style media="screen">
  .complete {
    width: 25% !important;
    transition-duration: 2s;
  }
</style>

<script type="text/javascript">
  function newInvite() {
    $('#form-invite').html("<%= j(render partial: 'users/dashboard/tabs/earn_points', locals: {invitation: Invitation.new}) %>");
  }
  $(document).ready(function() {
    $('.progress-bar').addClass("complete");
  });
</script>
