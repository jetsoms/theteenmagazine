<style type="text/css">
  .content_wrapper {
    max-width:670px;
    background:#fff;
    border-radius:3px;
    text-align:center;
    -webkit-box-shadow:0 6px 18px 0 rgba(0,0,0,.06);
    -moz-box-shadow:0 6px 18px 0 rgba(0,0,0,.06);
    box-shadow:0 6px 18px 0 rgba(0,0,0,.06);
  }
  .content p {
      color:#455056;
      font-size:15px;
      font-family: 'Rubik',sans-serif;
      line-height:24px;
      margin: 12px 0;
      text-align: left !important;
  }
  .bottom_links a {
    font-family: 'Open Sans', sans-serif !important;
    text-decoration: underline !important;
  }
  .bottom_links a:hover, .content a:hover {
    text-decoration: underline !important;
  }
  .padding {
    padding:0 35px;
  }
  .content img {
    max-width: 100%;
    height: auto !important;
  }
  .article {
    display: flex;
    flex-direction: row;
  }
  .article_image {
    display: block;
    width: 40%;
  }
  .article_image img {
    width: 100%;
  }
  .article_info {
    width: 60%;
  }
  .article_info h3 {
    font-size: 20px;
  }
  @media only screen and (max-width: 640px) {
    .padding {
      padding: 0 15px;
    }
    .content_wrapper {
      border-radius: 15px;
    }
  }
</style>
